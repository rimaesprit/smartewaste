  <!-- Bootstrap Bundle with Popper -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

  <script>
    document.addEventListener('DOMContentLoaded', function() {
      // Changement de l'apparence du header au défilement
      const header = document.querySelector('#header');
      const scrolled = () => {
        if (window.scrollY > 100) {
          header.classList.add('scrolled');
        } else {
          header.classList.remove('scrolled');
        }
      };
      window.addEventListener('load', scrolled);
      document.addEventListener('scroll', scrolled);
      
      // Menu mobile
      const mobileNavToggle = document.querySelector('.mobile-nav-toggle');
      const navbar = document.querySelector('.navbar ul');
      
      if (mobileNavToggle) {
        mobileNavToggle.addEventListener('click', function(e) {
          navbar.classList.toggle('active');
          this.classList.toggle('bi-list');
          this.classList.toggle('bi-x');
        });
      }
      
      // Navigation déroulante sur mobile
      const navDropdowns = document.querySelectorAll('.navbar .dropdown > .dropdown-toggle');
      navDropdowns.forEach(dropdown => {
        dropdown.addEventListener('click', function(e) {
          if (window.innerWidth < 992) {
            e.preventDefault();
            e.stopPropagation();
            
            // Fermer tous les autres menus déroulants
            navDropdowns.forEach(otherDropdown => {
              if (otherDropdown !== this) {
                otherDropdown.classList.remove('active');
                otherDropdown.nextElementSibling.classList.remove('show');
              }
            });
            
            this.classList.toggle('active');
            this.nextElementSibling.classList.toggle('show');
          }
        });
      });
      
      // Fermer le menu mobile lors du clic sur un lien
      document.querySelectorAll('.navbar .nav-link:not(.dropdown-toggle)').forEach(navLink => {
        navLink.addEventListener('click', () => {
          if (navbar.classList.contains('active')) {
            navbar.classList.remove('active');
            mobileNavToggle.classList.add('bi-list');
            mobileNavToggle.classList.remove('bi-x');
          }
        });
      });
      
      // Cliquer à l'extérieur ferme le menu
      document.addEventListener('click', function(e) {
        const isDropdownButton = e.target.closest('.dropdown-toggle');
        const isDropdownMenu = e.target.closest('.dropdown ul');
        
        if (!isDropdownButton && !isDropdownMenu) {
          navDropdowns.forEach(dropdown => {
            dropdown.classList.remove('active');
            dropdown.nextElementSibling.classList.remove('show');
          });
        }
      });

      // Add this script to ensure the dropdown works properly
      document.addEventListener('DOMContentLoaded', function() {
        // For desktop: handle hover dropdown
        if (window.innerWidth >= 992) {
          document.querySelectorAll('.navbar .nav-item.dropdown').forEach(function(item) {
            item.addEventListener('mouseenter', function() {
              this.querySelector('.dropdown-menu').classList.add('show');
            });
            item.addEventListener('mouseleave', function() {
              this.querySelector('.dropdown-menu').classList.remove('show');
            });
          });
        }
        
        // For mobile: ensure the dropdown toggle works with click
        document.querySelectorAll('.dropdown-toggle').forEach(function(dropdown) {
          dropdown.addEventListener('click', function(e) {
            if (window.innerWidth < 992) {
              e.preventDefault();
              e.stopPropagation();
              const menu = this.nextElementSibling;
              if (menu.classList.contains('show')) {
                menu.classList.remove('show');
              } else {
                document.querySelectorAll('.dropdown-menu.show').forEach(function(openMenu) {
                  openMenu.classList.remove('show');
                });
                menu.classList.add('show');
              }
            }
          });
        });
      });
    });
  </script>

  {% block javascripts %}{% endblock %}


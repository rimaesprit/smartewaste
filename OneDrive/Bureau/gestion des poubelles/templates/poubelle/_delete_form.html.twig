<form method="post" action="{{ path('app_poubelle_delete', {'id': poubelle.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette poubelle?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ poubelle.id) }}">
    <button class="btn btn-danger">
        <i class="fas fa-trash"></i> Supprimer la poubelle
    </button>
</form> 